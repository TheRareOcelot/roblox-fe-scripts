   loadstring(game:HttpGet("https://pastebin.com/raw/6nXuRhVV", true))()
end)
Section:NewButton("Eat Your Hats", "Teleports players to the nearest tool", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/6nXuRhVV", true))()

end)
Section:NewButton("Hats On a Leash", "Teleports players to the nearest tool", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRareOcelot/test/main/Hat%20On%20A%20Leash"))()

end)
