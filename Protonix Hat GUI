local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Protonix Hat GUI", "Synapse")
local Tab = Window:NewTab("Hat GUI")
local Section = Tab:NewSection("Main")
Section:NewButton("Floating Hats/Hat Orbit", "Teleports players to the nearest tool", function()
