local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Protonix Tool GUI", "Synapse")
local Tab = Window:NewTab("Tool GUI")
local Section = Tab:NewSection("Main")
Section:NewButton("Grab Tools", "Teleports players to the nearest tool", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/TheRareOcelot/test/main/Grab%20Tools', true))()
end)
Section:NewButton("Drop Tools", "Drops tool", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RoboGrape/dropbuttongui/main/dropitemgui"))()
end)
Section:NewButton("Tool Giver V2", "Gives Tools", function()
loadstring(game:HttpGet('https://pastebin.com/raw/Fj8KWmLC'))()
end)
Section:NewButton("Dupe Tools", "Dupe Tools", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Dupe%20Tools%20Main%20Script.md'))()
end)
Section:NewButton("Dupe Tools On Ground", "Dupes Tools On Ground", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Spam%20Tools'))()
end)
Section:NewButton("Tools On a Leash", "Tools On a Leash", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRareOcelot/test/main/Tools%20On%20a%20Leash"))()
end)
Section:NewButton("Unequip Tools", "Unequip Tools", function()
game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
end)
Section:NewButton("Invisible Handle/Tool", "Its an invisible tool", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRareOcelot/test/main/Invisible%20Tool"))()
end)
