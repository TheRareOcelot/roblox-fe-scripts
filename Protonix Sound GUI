local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Protonix Sound GUI", "Synapse")
local Tab = Window:NewTab("Sound GUI")
local Section = Tab:NewSection("Main")
Section:NewButton("Play All Sounds", "Play All Sounds", function()
        for _, v in pairs(cont:FindFirstChild("Sounds"):GetChildren()) do
            v:Play()
        end
end)

Section:NewButton("Stop All Sounds", "Stop All Sounds", function()
        for _, v in pairs(cont:FindFirstChild("Sounds"):GetChildren()) do
            v:Stop()
        end
end)

Section:NewButton("Oof Spammer", "Oof Spammer", function()
           loadstring(game:HttpGet("https://pastebin.com/raw/JaMbzp0B", true))()
 
end)
Section:NewButton("Sound GUI", "Sound GUI", function()
           loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRareOcelot/test/main/Sound%20GUI", true))()
 
end)
