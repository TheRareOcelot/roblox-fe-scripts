local Toggle = Tab:CreateToggle({
    Name = "Freefalling",
    CurrentValue = true,
    Flag = "Tofurye1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
      game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
       
        
        if  game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            if Value then
                game.Players.LocalPlayer:SetAttribute("Freefalling", true)
                spawn(function()
                    while game.Players.LocalPlayer:GetAttribute("Freefalling") and game:GetService("RunService").Stepped:Wait() do
                        game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Freefall)
                    end
                end)
            else
               
                game.Players.LocalPlayer:SetAttribute("Freefalling", false)
                game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.GettingUp)
                 game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Running)
            end
        end
    end,
})
